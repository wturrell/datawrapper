{% extends "chart/base.twig" %}

{% block content %}

{{ parent() }}


<script type="text/javascript">
dw.backend.ready(function() {

    require(['dw/chart/upload'], function(upload) {
        upload.init();
    });

    require(['dw/chart/describe'], function(describe) {
        describe.init("{{ chartLocale }}", {
            columnType: "{% trans 'Column type' %}",
            inputFormat: "{% trans 'Input format' %}",
            auto: "{% trans 'Automatic' %}",
            text: "{% trans 'Text' %}",
            number: "{% trans 'Number' %}",
            date: "{% trans 'Date' %}"
        });
    });

});
</script>

